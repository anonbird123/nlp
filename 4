import nltk
from nltk import CFG
g = CFG.fromstring("""
S -> NP VP
NP -> Det N | Det Adj N | Det Adj Adj N
VP -> V NP | V NP PP | V NP
PP -> P NP
Det -> 'the' | 'a'
Adj -> 'quick' | 'brown' | 'lazy' | 'small'
N -> 'fox' | 'dog' | 'cat' | 'park'
V -> 'jumps' | 'runs' | 'sleeps' | 'sees'
P -> 'in' | 'on' | 'over'
""")
p = nltk.ChartParser(g)
for t in p.parse(['the','quick','brown','fox','sees','a','dog']):
    print("\nTree:\n", t); t.pretty_print()
